# SharePoint to Blob Sync with AI Search Integration
# ===================================================
# Copy this file to .env and fill in your values

# ==========================
# Authentication
# ==========================
AZURE_CLIENT_ID=your-client-id
AZURE_CLIENT_SECRET=your-client-secret
AZURE_TENANT_ID=your-tenant-id

# ==========================
# SharePoint Configuration
# ==========================
SHAREPOINT_SITE_URL=https://contoso.sharepoint.com/sites/MySite
SHAREPOINT_DRIVE_NAME=Documents
SHAREPOINT_FOLDER_PATH=/

# ==========================
# Azure Blob Storage
# ==========================
AZURE_STORAGE_ACCOUNT_NAME=yourstorageaccount
AZURE_BLOB_CONTAINER_NAME=sharepoint-sync
AZURE_BLOB_PREFIX=

# ==========================
# Sync Settings
# ==========================
DELETE_ORPHANED_BLOBS=true
DRY_RUN=false
SYNC_PERMISSIONS=true

# ==========================
# Azure AI Search
# ==========================
SEARCH_SERVICE_NAME=your-search-service
SEARCH_RESOURCE_GROUP=your-resource-group
SEARCH_API_KEY=your-api-key
API_VERSION=2025-11-01-preview

# Component names
INDEX_NAME=sharepoint-sync-index
INDEXER_NAME=sharepoint-sync-indexer
SKILLSET_NAME=sharepoint-sync-skillset
DATASOURCE_NAME=sharepoint-sync-datasource

# ==========================
# Azure OpenAI
# ==========================
OPENAI_RESOURCE_URI=https://your-openai.openai.azure.com
EMBEDDING_DEPLOYMENT_ID=text-embedding-3-large
EMBEDDING_MODEL_NAME=text-embedding-3-large
EMBEDDING_DIMENSIONS=3072

# ==========================
# Azure Subscription
# ==========================
SUBSCRIPTION_ID=your-subscription-id
